URxvt.font: xft:Fira Code:size=11
*.internalBorder: 0

! real transparency (composite)
URxvt*depth: 32
urxvt*tintColor: #000000
! fake transparency
!URxvt.transparent: true
!URxvt.shading: 20

URxvt*termName: rxvt-unicode-256color

! remove scroll bar
URxvt.scrollBar: false

! scroll in relation to buffer (with mouse scroll or Shirt+Page Up)
URxvt*scrollWithBuffer: True

! Clickable URLs
URxvt.perl-ext-common: default,matcher,font-size
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1

! enable ctrl+arrow
URxvt.keysym.Control-Up:     \033[1;5A
URxvt.keysym.Control-Down:   \033[1;5B
URxvt.keysym.Control-Left:   \033[1;5D
URxvt.keysym.Control-Right:  \033[1;5C

! scroll up or down with keys
URxvt.keysym.Shift-Up: command:\033]720;1\007
URxvt.keysym.Shift-Down: command:\033]721;1\007


! disable Ctrl+Shift default bindings
URxvt.iso14755: false
URxvt.iso14755_52: false

