URxvt.font: xft:hack:size=11
URxvt*depth: 32

! remove scroll bar
URxvt.scrollBar: false

! scroll in relation to buffer (with mouse scroll or Shirt+Page Up)
URxvt*scrollWithBuffer: True

! Clickable URLs
URxvt.perl-ext-commin: default,matcher
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1

! Snazzy - Converted from https://github.com/sindresorhus/iterm2-snazzy
!          using https://github.com/richo/itermcolors2xdefaults
#URxvt*background:   [90]#1e1f28
URxvt*background:   [95]#000000
URxvt*foreground:   #ebece6
URxvt*cursorColor:   #e4e4e4
URxvt*color0:   #000
URxvt*color1:   #fb4245
URxvt*color2:   #50fa7b
URxvt*color3:   #f0fa8b
URxvt*color4:   #49b9fe
URxvt*color5:   #fb4cb3
URxvt*color6:   #8be9fd
URxvt*color7:   #ededec
URxvt*color8:   #555555
